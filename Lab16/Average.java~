public class Average{
   public static void main(String[] args){
      int[][] table = {{1,2,3},{4,5,6},{7,8}};
      for (int[] row: table){
         float total = 0;
         for (int num: row){
            System.out.print(num + " ");
            total += num;
         }
      System.out.println("\tAverage : " + (total/row.length));
      }
   }
}